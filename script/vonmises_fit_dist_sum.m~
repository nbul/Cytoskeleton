%% Load distribution data

clear y_forfit1 x bin counter kappa SD mu p alpha mu_degrees

stretch_factor = 0.5;

y_forfit1 = m_added_norm;
x = y_forfit1(:,1);
x = x /90 * pi;
bin = 0.14;
counter = size(y_forfit1);

for fit=2:counter(2)
y_forfit = y_forfit1(:,fit);
%% Von Mises Fit
y = y_forfit;
kappa(fit) = circ_kappa(x,y,bin);
SD(fit) = sqrt(1/kappa(fit)) * (180/pi) * stretch_factor;
mu(fit) = circ_mean(x,y);
[p alpha] = circ_vmpdf(x,mu(fit),kappa(fit));
mu_degrees(fit) = mu(fit) * (180/pi);

end

SD_average = sum(SD)/length(SD);
sem=std(SD)/sqrt(length(SD));
cd('../');







    








